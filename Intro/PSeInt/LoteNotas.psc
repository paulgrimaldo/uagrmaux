Algoritmo LoteNotas
	Definir n Como Entero;
	Definir nota Como Real; 
	
	Escribir "Introduzca la cantidad de notas a leer";
	Leer n;
	
	Mientras n > 0 Hacer
		Escribir "Introduzca la nota";
		Leer nota; 
		Escribir  "Nota: "
		Escribir nota; 
		
		n<-n-1;
	FinMientras
	Escribir "FIN";
	
FinAlgoritmo
